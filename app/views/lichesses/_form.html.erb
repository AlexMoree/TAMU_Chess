  </div>

  <div class="field">
    <%= form.label :rank %>
    <%= form.number_field :rank %>
  </div>

  <div class="field">
    <%= form.label :total_played %>
    <%= form.number_field :total_played %>
  </div>

  <div class="field">
    <%= form.label :win %>
    <%= form.number_field :win %>
  </div>

  <div class="field">
    <%= form.label :draw %>
    <%= form.number_field :draw %>
  </div>

  <div class="field">
    <%= form.label :loss %>
    <%= form.number_field :loss %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
