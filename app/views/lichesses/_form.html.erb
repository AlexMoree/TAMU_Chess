<%= form_with(model: lichess) do |form| %>
  <% if lichess.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(lichess.errors.count, "error") %> prohibited this lichess from being saved:</h2>

      <ul>
        <% lichess.errors.each do |error| %>
          <li><%= error.full_message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :student_id %>
    <%= form.number_field :student_id %>
  </div>

  <div class="field">
    <%= form.label :username %>
