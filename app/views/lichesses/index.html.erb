<p id="notice"><%= notice %></p>

<h1>Lichesses</h1>

<table>
  <thead>
    <tr>
      <th>Username</th>
      <th>Blitz</th>
      <th>Rapid</th>
      <th>Total Played</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lichesses.each do |lichess| %>
      <tr>
        <td><%= lichess.lichess_com_username %></td>
        <td><%= lichess.blitz %></td>
        <td><%= lichess.rapid %></td>
        <td><%= lichess.total_played %></td>
        <td><%= link_to 'Show', lichess %></td>
        <td><%= link_to 'Edit', edit_lichess_path(lichess) %></td>
        <td><%= link_to 'Destroy', lichess, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Update', lichesses_path %>

<br>

<%= link_to 'Back', personal_informations_path %>